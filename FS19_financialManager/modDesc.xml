			<en>FinancialManager 0.0.0.1</en>
			<nl>FinancialManager 0.0.0.1</nl>	
		</text>
	</l10n>
	
	<actions>
		<action name="fmHomeScreen"/>
	</actions>
	<inputBinding>
		<actionBinding action="fmHomeScreen">	
			<binding device="KB_MOUSE_DEFAULT" input="KEY_lctrl KEY_f" axisComponent="+" inputComponent="+" index="1"/>
		</actionBinding>	
	</inputBinding>
	
	<extraSourceFiles>
		<sourceFile filename="fmGui.lua" />
		<sourceFile filename="scripts/fmMain.lua" />
	</extraSourceFiles>
  
</modDesc>